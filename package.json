{
  "name": "jupyterlab-simpledark",
  "version": "3.0.3",
  "description": "A JupyterLab simple and dark theme.",
  "author": {
    "name": "ericmiguel",
    "email": "10134699+ericmiguel@users.noreply.github.com"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab",
    "simpledark"
  ],
  "license": "BSD-3-Clause",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "schema/*.json",
    "style/*",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc",
    "build:webpack": "webpack",
    "clean": "rimraf lib && rimraf static && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w",
    "watch:webpack": "webpack --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.7",
    "@jupyterlab/apputils": "^3.0.5",
    "@jupyterlab/settingregistry": "^3.0.5"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mini-css-extract-plugin": "~0.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "svg-url-loader": "~5.0.0",
    "svgo": "~1.3.2",
    "svgo-loader": "~2.2.1",
    "typedoc": "~0.17.6",
    "typescript": "~4.1.3",
    "url-loader": "~4.1.0",
    "watch": "^0.13.0",
    "webpack": "~4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themePath": "style/index.css",
    "outputDir": "jupyterlab-simpledark/labextension",
    "schemaDir": "schema"
  },
  "homepage": "https://github.com/ericmiguel/jupyterlab-simpledark",
  "bugs": {
    "url": "https://github.com/ericmiguel/jupyterlab-simpledark/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericmiguel/jupyterlab-simpledark"
  },
  "styleModule": "style/index.js"
}
